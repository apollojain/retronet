{
  "manifest_version": 2,
  "name": "RetroNet",
  "description": "See the internet as your forefathers did.",
  "version": "0.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
   "default_icon": "icon16.png",
   "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["sweetalert.css"],
      "js": ["sweetalert.min.js", "myscript.js"]
    }
  ]
}